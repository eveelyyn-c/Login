<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Iniciarsesion">
    <EditText
        android:id="@+id/correo1"
        android:layout_width="274dp"
        android:layout_height="48dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="71dp"
        android:ems="10"
        android:hint=" Correo"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/contrasena1"
        android:layout_width="274dp"
        android:layout_height="48dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="71dp"
        android:ems="10"
        android:hint="ContraseÃ±a"
        android:inputType="textPassword"
        android:minHeight="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/correo1"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/iniciarsesionbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="131dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:onClick="iniciarSesion"
        android:shadowRadius="@android:dimen/app_icon_size"
        android:text="@string/btn_iniciar_sesion"
        app:circularflow_defaultRadius="@android:dimen/app_icon_size"
        app:cornerRadius="@android:dimen/app_icon_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasena1"
        tools:text="@string/btn_iniciar_sesion" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="148dp"
        android:layout_marginEnd="111dp"
        android:text="@string/inicar"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>